{
  "name": "sukhrobnurali/querylite",
  "description": "QueryLite - A Lightweight SQL Query Builder",
  "type": "library",
  "license": "MIT",
  "require": {
    "php": ">=8.1",
    "ext-pdo_mysql": "*",
    "ext-pdo": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^10"
  },
  "autoload": {
    "psr-4": {
      "QueryLite\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "QueryLite\\Tests\\": "tests/"
    }
  },
  "scripts": {
    "test": "vendor/bin/phpunit"
  }
}
